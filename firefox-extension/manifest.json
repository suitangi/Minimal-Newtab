{
  "manifest_version": 2,
  "name": "Minimal Newtab",
  "version": "1.0.0",
  "short_name": "Min Newtab",
  "description": "Replaces the newtab with ...",
  "permissions": ["storage", "bookmarks"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
